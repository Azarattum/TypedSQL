{
  "name": "@vlcn.io/vscode-sql-literal",
  "displayName": "SQL tagged template literals (syntax only)",
  "description": "Syntax highlighting for SQL in template literals tagged with `sql` or `sqlFrag` or `schema`",
  "version": "0.1.0",
  "license": "MIT",
  "publisher": "vlcn.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlcn-io/typed-sql/packages/tmlanguage"
  },
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.svelte",
          "source.ts",
          "source.tsx"
        ],
        "scopeName": "inline.tagged-template-sql",
        "path": "./syntaxes/grammar.json",
        "embeddedLanguages": {
          "meta.embedded.block.sql": "sql"
        }
      }
    ]
  }
}
